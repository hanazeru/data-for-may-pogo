="https://iv4u.lima-city.de/?l=en&amp;p=Mew" onclick="PkmClick(&#39;Mew&#39;);">Mew</a></td><td>210</td><td>210</td><td style="border-right: 1px solid #5a5a5a;padding-top:0px;">225</td><td style="border-right: 1px solid #5a5a5a;padding-top:0px;">3691<span class="kp">/201</span></td><td><img src="./IV4U - Pokémon List_files/psychic.png" height="14" width="62" style="vertical-align:middle;cursor:pointer;" onclick="rbTypeClick(&quot;typePsycho&quot;, true);"></td></tr><tr class="tr-hover"><td style="padding-top:5px;">199</td><td style="border-right: 1px solid #5a5a5a;padding-top:5px;" class="kp">Kanto</td><td style="text-align:left;padding-left:5px;border-right: 1px solid #5a5a5a;padding-top:5px;"><a style="text-decoration:underline;cursor:pointer;color:black;" href="https://iv4u.lima-city.de/?l=en&amp;p=GalarianSlowking" onclick="PkmClick(&#39;Galar-Laschoking&#39;);"><span class="kp">Galarian</span> Slowking</a></td><td style="padding-top:5px;">190</td><td style="padding-top:5px;">180</td><td style="border-right: 1px solid #5a5a5a;padding-top:5px;">216</td><td style="border-right: 1px solid #5a5a5a;padding-top:5px;">3072<span class="kp">/194</span></td><td style="padding-top:5px;"><img src="./IV4U - Pokémon List_files/poison.png" height="14" width="62" style="vertical-align:middle;cursor:pointer;" onclick="rbTypeClick(&quot;typeGift&quot;, true);"><img src="./IV4U - Pokémon List_files/psychic.png" height="14" width="62" style="vertical-align:middle;padding-left:5px;cursor:pointer;" onclick="rbTypeClick(&quot;typePsycho&quot;, true);"></td></tr></table></div>
</td></tr></tbody></table>
<script type="text/javascript">
	var working = false;
	var sort = 0;
	var en = true;
	var hide = false;

	for (var i = 0; i < 11; i += 2) {
		document.getElementById('col' + i).setAttribute('original_style', document.getElementById('col' + i).getAttribute('style'));
	}

	function sortClick(col) {
		if (working) {return false;}
		working = true;

		if (col < 3) {
			if (sort == col) {
				sort = col + 1;
			} else {
				sort = col;
			}
		} else {
			if (sort == col + 1) {
				sort = col;
			} else {
				sort = col + 1;
			}
		}

		updateList();
	}

	function rbGenClick(rbID, update) {
		if (working) {return false;}
		working = true;
		for (var i = 0; i < genList.length; i++) {
			if (document.getElementById('gen' + genList[i])) {
				document.getElementById('gen' + genList[i]).checked = 'gen' + genList[i] == rbID;
			}
		}
		if (document.getElementById('genAll')) {
			document.getElementById('genAll').checked = 'genAll' == rbID;
		}
		if (update) {
			updateList();	
		} else {
			working = false;
		}
	}

	function rbAglClick(rbID, update) {
		if (working) {return false;}
		working = true;
		var IDs = ['aglAll', 'aglAlolan', 'aglGalarian', 'aglLegend'];
		for (var i = 0; i < IDs.length; i++) {
			if (document.getElementById(IDs[i])) {
				document.getElementById(IDs[i]).checked = IDs[i] == rbID;
			}
		}
		if (update) {
			updateList();	
		} else {
			working = false;
		}	
	}

	function rbTypeClick(rbID, update) {
		if (working) {return false;}
		working = true;
		var IDs = ['typeAll', 'typeWasser', 'typeNormal', 'typePflanze', 'typePsycho', 'typeFlug', 'typeKäfer', 'typeFeuer', 'typeGift', 'typeBoden', 'typeUnlicht', 'typeKampf', 'typeElektro', 'typeDrache', 'typeGestein', 'typeStahl', 'typeGeist', 'typeFee', 'typeEis'];
		for (var i = 0; i < IDs.length; i++) {
			if (document.getElementById(IDs[i])) {
				document.getElementById(IDs[i]).checked = IDs[i] == rbID;
			}
		}
		if (update) {
			updateList();	
		} else {
			working = false;
		}	
	}

	function btnResetListClick() {
		document.getElementById("aglAll").checked = true;
		rbAglClick("aglAll", false);
		document.getElementById("typeAll").checked = true;
		rbTypeClick("typeAll", false);
		document.getElementById("genAll").checked = true;
		rbGenClick("genAll", true);
	}

	function areRelated(id1, id2, id1relations) {
		if (id1 % 10000 == id2 % 10000 || id1 == 0 || id2 == 0) {
			return true;
		}
		for (var i = 0; i < id1relations.split(',').length; i++) {
			if (id2 % 10000 == id1relations.split(',')[i] % 10000) {
				return true;
			}
		}
		return false;
	}

	function typeIcon(type) {
		if (!en) {
			return type.toLowerCase().replace('ä', 'ae') + '.png';
		}
		switch(type) {
		  case 'Wasser': return 'water.png';
		  case 'Normal': return 'normal.png';
		  case 'Flug': return 'flying.png';
		  case 'Pflanze': return 'grass.png';
		  case 'Psycho': return 'psychic.png';
		  case 'Käfer': return 'bug.png';
		  case 'Boden': return 'ground.png';
		  case 'Gift': return 'poison.png';
		  case 'Feuer': return 'fire.png';
		  case 'Gestein': return 'rock.png';
		  case 'Unlicht': return 'dark.png';
		  case 'Elektro': return 'electric.png';
		  case 'Kampf': return 'fighting.png';
		  case 'Stahl': return 'steel.png';
		  case 'Eis': return 'ice.png';
		  case 'Drache': return 'dragon.png';
		  case 'Fee': return 'fairy.png';
		  case 'Geist': return 'ghost.png';
		}
		return '';
	}

	function lightStr(str) {
		var myStr = str;
		//myStr = myStr.replaceAll('', '');
		myStr = myStr.replaceAll('ü', 'ue');
		myStr = myStr.replaceAll('ä', 'ae');
		myStr = myStr.replaceAll('ö', 'oe');
		myStr = myStr.replaceAll('Ü', 'Ue');
		myStr = myStr.replaceAll('Ä', 'Ae');
		myStr = myStr.replaceAll('Ö', 'Oe');
		myStr = myStr.replaceAll('ß', 'ss');
		myStr = myStr.replaceAll('é', 'e');
		myStr = myStr.replaceAll('è', 'e');
		myStr = myStr.replaceAll('á', 'a');
		myStr = myStr.replaceAll('à', 'a');
		myStr = myStr.replaceAll('É', 'e');
		myStr = myStr.replaceAll('È', 'e');
		myStr = myStr.replaceAll('Á', 'a');
		myStr = myStr.replaceAll('À', 'a');
		myStr = myStr.replaceAll('\'', '');

		var myChars = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-';
		var i = 0;
		while (i < myStr.length) {
			if (myChars.indexOf(myStr[i].toUpperCase()) == -1) {
				myStr = myStr.replaceAll(myStr[i], '');
			} else {
				i++;
			}
		}
		return myStr;
	}

	function PkmClick(pkm) {
		qr = new XMLHttpRequest();
		qr.open('get','.?s=,,,,' + pkm + '');
		qr.send();
		if (true) { //xxx
			gtag('event', 'List', { 'event_category': 'Choose Pokemon', 'event_label': pkm});
		}
	}

	function addToListTable(pkmRow, prevID) {
		var listTable = document.getElementById("listTable");
		var newRow = listTable.insertRow(listTable.rows.length);
		newRow.setAttribute('class', 'tr-hover');
		var paddingTop = 5;
		if (areRelated(pkmRow[1], prevID, pkmRow[12]) && prevID != 0) {paddingTop = 0;}
		var cell = newRow.insertCell(0);
		cell.innerHTML = pkmRow[1] % 10000;
		if (paddingTop > 0) {cell.setAttribute('style', 'padding-top:' + paddingTop + 'px;');}
		cell = newRow.insertCell(1);
		cell.innerHTML = (pkmRow[9] == 'Einall' && en ? 'Unova' : (pkmRow[9] != '7. Generation' ? pkmRow[9] : ''));
		cell.setAttribute('style', 'border-right: 1px solid #5a5a5a;padding-top:' + paddingTop + 'px;' + (listTable.rows.length == 2 ? 'min-width:52px;' : ''));
		if (document.getElementById('gen' + pkmRow[9]) && document.getElementById('gen' + pkmRow[9]).checked == false) {
			cell.setAttribute('style', 'border-right: 1px solid #5a5a5a;padding-top:' + paddingTop + 'px;cursor:pointer;text-decoration:underline;' + (listTable.rows.length == 2 ? 'min-width:52px;' : ''));
			cell.setAttribute('onclick', 'rbGenClick("gen' + pkmRow[9] + '", true);');
		}
		cell.setAttribute('class', 'kp');
		cell = newRow.insertCell(2);
		cell.setAttribute('style', 'text-align:left;padding-left:5px;border-right: 1px solid #5a5a5a;padding-top:' + paddingTop + 'px;');
		var a = document.createElement('a');
		a.innerHTML = shortenPkm((en ? pkmRow[3] : pkmRow[2]), en);
		a.setAttribute('style', 'text-decoration:underline;cursor:pointer;color:black;');
		a.setAttribute('href', '../?' + (en ? 'l=en&' : '') + 'p=' + lightStr((en ? pkmRow[3] : pkmRow[2])) + (hide ? '&hide' : ''));
		a.setAttribute('onclick', 'PkmClick(\'' + lightStr(pkmRow[2]) + '\');');
		cell.appendChild(a);
		cell = newRow.insertCell(3);
		cell.innerHTML = pkmRow[4];
		if (paddingTop > 0) {cell.setAttribute('style', 'padding-top:' + paddingTop + 'px;');}
		cell = newRow.insertCell(4);
		cell.innerHTML = pkmRow[5];
		if (paddingTop > 0) {cell.setAttribute('style', 'padding-top:' + paddingTop + 'px;');}
		cell = newRow.insertCell(5);
		cell.innerHTML = pkmRow[6];
		cell.setAttribute('style', 'border-right: 1px solid #5a5a5a;padding-top:' + paddingTop + 'px;');
		cell = newRow.insertCell(6);
		cell.innerHTML = pkmRow[10];
		cell.setAttribute('style', 'border-right: 1px solid #5a5a5a;padding-top:' + paddingTop + 'px;');
		var span = document.createElement('span');
		span.setAttribute('class', 'kp');
		span.innerHTML = '/' + pkmRow[11];
		cell.appendChild(span);
		cell = newRow.insertCell(7);
		if (paddingTop > 0) {cell.setAttribute('style', 'padding-top:' + paddingTop + 'px;');}
		var img = document.createElement('img');
		img.setAttribute('src', '../type/' + typeIcon(pkmRow[7]));
		img.setAttribute('height', '14');
		img.setAttribute('width', '62');	
		img.setAttribute('style', 'vertical-align:middle;');		
		if (!document.getElementById('type' + pkmRow[7]) || document.getElementById('type' + pkmRow[7]).checked == false) {				
			img.setAttribute('onclick', 'rbTypeClick("type' + pkmRow[7] + '", true);');
			img.setAttribute('style', 'vertical-align:middle;cursor:pointer;');
		}
		cell.appendChild(img);
		if (pkmRow[8] != '') {
			img = document.createElement('img');
			img.setAttribute('src', '../type/' + typeIcon(pkmRow[8]));
			img.setAttribute('height', '14');
			img.setAttribute('width', '62');
			img.setAttribute('style', 'vertical-align:middle;padding-left:5px;');
			if (!document.getElementById('type' + pkmRow[8]) || document.getElementById('type' + pkmRow[8]).checked == false) {				
				img.setAttribute('onclick', 'rbTypeClick("type' + pkmRow[8] + '", true);');
				img.setAttribute('style', 'vertical-align:middle;padding-left:5px;cursor:pointer;');
			}
			cell.appendChild(img);
		}
	}

	function updateList(fstRun = true) {		
		working = true;

		document.getElementById('countResults').innerHTML = (en ? 'Searching for Pok&eacute;mon...' : 'Pok&eacute;mon werden gesucht...');
		document.getElementById('btnResetList').setAttribute('style', 'background-color:silver;cursor:default;height:25px;width:60px;padding:0px;');
		document.getElementById('btnResetList').setAttribute('onclick', '');

		var newTable = document.createElement('table');
		newTable.setAttribute('style', 'text-align:center;border-collapse:collapse;');
		newTable.setAttribute('class', 'proof');
		newTable.appendChild(document.getElementById('fstRow'));
		document.getElementById('listTable').remove();
		newTable.setAttribute('id', 'listTable');
		document.getElementById('content').appendChild(newTable);

		if (fstRun) {			
			var cell = document.getElementById("listTable").insertRow(1).insertCell(0);
			cell.setAttribute('colspan', '9');
			cell.innerHTML = (en ? 'Searching for Pok&eacute;mon...' : 'Pok&eacute;mon werden gesucht...');
			setTimeout(function(){ updateList(false); }, 1);
			return false;
		}

		for (var i = 0; i < 11; i += 2) {
			if (document.getElementById('col' + i)) {
				if (i != sort - (sort % 2)) {
					document.getElementById('col' + i).setAttribute('style', document.getElementById('col' + i).getAttribute('original_style'));
				} else {
					document.getElementById('col' + i).setAttribute('style', 'background: url("../img/bgX' + ((sort < 4 && sort % 2 == 1) || (sort >= 4 && sort % 2 == 0) ? '_' : '') + '.png"); background-size: 100% 100%;' + document.getElementById('col' + i).getAttribute('original_style'));					
				}
			}
		}

		var aglAll = document.getElementById('aglAll').checked;
		var aglAlolan = document.getElementById('aglAlolan').checked;
		var aglGalarian = document.getElementById('aglGalarian').checked;
		var aglLegend = document.getElementById('aglLegend').checked;

		var genAll = document.getElementById('genAll').checked;
		var genSel = '';
		for (var i = 0; i < genList.length; i++) {
			if (document.getElementById('gen' + genList[i]).checked) {
				genSel = genList[i];
				break;
			}
		}

		var types = ['Wasser', 'Normal', 'Pflanze', 'Psycho', 'Flug', 'Käfer', 'Feuer', 'Gift', 'Boden', 'Unlicht', 'Kampf', 'Elektro', 'Drache', 'Gestein', 'Stahl', 'Geist', 'Fee', 'Eis'];
		var typeAll = document.getElementById('typeAll').checked;
		var typeSel = '';
		for (var i = 0; i < types.length; i++) {
			if (document.getElementById('type' + types[i]) && document.getElementById('type' + types[i]).checked) {
				typeSel = types[i];
				break;
			}
		}

		qr = new XMLHttpRequest();
		qr.open('get','.?s=' + (aglAlolan + 2*aglGalarian + 3*aglLegend) + ',' + sort + ',' + typeSel + ',' + genSel + '');
		qr.send();

		var filterList = list.filter(function(a) {
			if (!aglAll
				&& (!aglAlolan || a[14] == '0')
				&& (!aglGalarian || a[15] == '0')
				&& (!aglLegend || a[13] == '0')) {
				return false;
			}
			if (!genAll && a[9] != genSel) {
				return false;
			}
			if (!typeAll && a[7] != typeSel && a[8] != typeSel) {
				return false;
			}
			return true;
		});

		filterList.sort( function(a, b) {
			// Nummer
			if (Math.floor(sort/2) == 0 && sort % 2 == 1) {
				if (b[1] % 10000 < a[1] % 10000) {return (sort % 2 == 0 ? 1 : -1);}
				if (a[1] % 10000 < b[1] % 10000) {return (sort % 2 == 1 ? 1 : -1);}
			}
			// Name
			if (Math.floor(sort/2) == 1) {
				if (en && a[3].localeCompare(b[3]) != 0) {
					return (sort % 2 == 0 ? a[3].localeCompare(b[3]) : b[3].localeCompare(a[3]));
				}
				if (!en && a[2].localeCompare(b[2]) != 0) {
					return (sort % 2 == 0 ? a[2].localeCompare(b[2]) : b[2].localeCompare(a[2]));
				}
			}
			// Angriff
			if (Math.floor(sort/2) == 2) {
				if (b[4] < a[4]) {return (sort % 2 == 0 ? 1 : -1);}
				if (a[4] < b[4]) {return (sort % 2 == 1 ? 1 : -1);}
			}
			// Verteidigung
			if (Math.floor(sort/2) == 3) {
				if (b[5] < a[5]) {return (sort % 2 == 0 ? 1 : -1);}
				if (a[5] < b[5]) {return (sort % 2 == 1 ? 1 : -1);}
			}
			// KP
			if (Math.floor(sort/2) == 4) {
				if (b[6] < a[6]) {return (sort % 2 == 0 ? 1 : -1);}
				if (a[6] < b[6]) {return (sort % 2 == 1 ? 1 : -1);}
			}
			// max WP
			if (Math.floor(sort/2) == 5) {
				if (b[10] < a[10]) {return (sort % 2 == 0 ? 1 : -1);}
				if (a[10] < b[10]) {return (sort % 2 == 1 ? 1 : -1);}
			}
			if (b[0] < a[0]) {return 1;}
			return -1;
		});

		var prevID = 0;
		for (var i = 0; i < filterList.length; i++) {
			addToListTable(filterList[i], prevID);
			prevID = filterList[i][1];
		}
		document.getElementById('countResults').innerHTML = filterList.length + ' Pok&eacute;mon ' + (en ? 'found' : 'gefunden');
		if (document.getElementById('genAll').checked && document.getElementById('typeAll').checked && document.getElementById('aglAll').checked) {
			document.getElementById('btnResetList').setAttribute('style', 'background-color:silver;cursor:default;height:25px;width:60px;padding:0px;');
			document.getElementById('btnResetList').setAttribute('onclick', '');
		} else {
			document.getElementById('btnResetList').setAttribute('style', 'cursor:pointer;height:25px;width:60px;padding:0px;');
			document.getElementById('btnResetList').setAttribute('onclick', 'btnResetListClick();');
		}

		if (document.getElementById('listTable').rows.length == 1) {
			var cell = document.getElementById("listTable").insertRow(1).insertCell(0);
			cell.setAttribute('colspan', '9');
			cell.innerHTML = (en ? 'No Pok&eacute;mon found' : 'Keine Pok&eacute;mon gefunden');
		}

		working = false;
	}

	window.onload = function () { updateList(); }
</script>



</body></html>
